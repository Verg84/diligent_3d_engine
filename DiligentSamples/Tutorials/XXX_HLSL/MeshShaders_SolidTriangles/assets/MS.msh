static const float2 Triangle_Vertices[]=
{
	{0.000,-1.000},		// 0
	{0.150,-0.500},		// 1
	{0.150,0.000},		// 2
	{0.150,0.000},		// 3
	{0.150,0.500},		// 4
	{1.000,1.000},		// 5
	{-0.300,1.000},		// 6
	{-0.615,0.500},		// 7
	{-0.165,0.00},		// 8
	{-0.615,0.00},		// 9
	{-0.615,-0.500},	// 10
	{-0.300,-1.00},		// 11
};

static const uint3 Triangle_Indices[]=
{	
	{11,10,2},
	{8,7,7},
	{0,0,8},
	{2,2,5},
	{10,2,10},
	{7,5,6},
	
};
struct MSOut
{
	float4 Position:SV_POSITION;
	float4 Color:COLOR0;
};

[numthreads(12,1,1)]
[outputtopology("triangle")]
void main(
	in uint I:SV_GroupThreadID,
	out indices uint3 tris[9],
	out vertices MSOut verts[12]
)
{	
	SetMeshOutputCounts(12,6);

	verts[I].Position=float4(Triangle_Vertices[I],0.0,1.0);
	verts[I].Color=float4(Triangle_Vertices[I],0.0,1.0);

	if(I<12)
	{
		tris[I]=Triangle_Indices[I];
	}
}